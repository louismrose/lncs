@INPROCEEDINGS{memmel:reiterer:inspector,
  author = {Memmel, T. and Reiterer, H.},
  title = {Inspector: Method and Tool for Visual UI Specification},
  booktitle = {Proc.\ Intl.\ IASTED Conf. Human Computer Interaction (IASTED-HCI 2008)},
  year = {2008},
  editor = {Cunliffe, D.},
  month = {March},
  organization = {IASTED},
  publisher = {Acta Press},
  abstract = {When the user interface (UI) has to be specified, a picture is worth
	a thousand words and the worst thing a human computer interaction
	(HCI) expert can do is attempt to write a natural language specification
	for it. Nevertheless, this practice is still common and it is therefore
	a difficult task to move from text-based requirements and problem
	space concepts to a final UI design, and then back again. Especially
	for the specification of interactive UIs, however, HCI experts must
	frequently switch between high-level descriptions and detailed screens.
	In our research we found that advanced UI specifications therefore
	have to be made up of interconnected artefacts that have distinct
	levels of abstraction. With regards to the transparency and traceability
	of the rationale of the UI specification, transitions and dependencies
	must be visual and traversable. We introduce a UI specification method
	that interactively integrates interdisciplinary and informal modelling
	languages with different fidelities of UI prototyping. With an innovative
	experimental tool, we finally assemble models and design to a visual
	UI specification that will quickly take the place of text-based artefacts.},
  owner = {stoerrle},
  timestamp = {2008.05.20}
}

